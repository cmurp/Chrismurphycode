<html>
<head>
    <% include ./partials/head %>
</head>
<body>
    <div class="container">
        <h1>Default</h1>
        <form method="post" action="/addpost">
            <div class="form-group">
              <label for="github">Github Link</label>
              <input class="form-control form-control-sm" name="github" type="text">
            </div>
            <div class="form-group">
              <label for="github">Tags</label>
              <span class="badge badge-default">default</span>
            </div>
          <div class="form-group">
            <label for="content">Project Details</label>
            <textarea class="form-control" style="height:750px;" name="content" rows="3"></textarea>
          </div>
          <div class="custom-control custom-checkbox">
              <input type="checkbox" class="custom-control-input" id="featured" name="featured" checked>
              <label class="custom-control-label" for="featured">Featured</label>
          </div>
          <input type="submit" class="btn btn-primary">
        </form>
    </div>
</body>
</html>
